<project>
    <actions/>
    <description/>
    <keepDependencies>false</keepDependencies>
    <properties>
        <hudson.model.ParametersDefinitionProperty>
            <parameterDefinitions>

                <hudson.model.StringParameterDefinition>
                    <name>gitUrl</name>
                    <description>git地址</description>
                    <defaultValue/>
                    <trim>true</trim>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>gitBranch</name>
                    <description>git分支</description>
                    <defaultValue/>
                    <trim>true</trim>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>jobName</name>
                    <description>job名称</description>
                    <defaultValue/>
                    <trim>true</trim>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>saveListUrl</name>
                    <description>批量保存测试用例接口</description>
                    <defaultValue>无</defaultValue>
                    <trim>true</trim>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>saveListToken</name>
                    <description>用户token</description>
                    <defaultValue/>
                    <trim>true</trim>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>userId</name>
                    <description>用户id</description>
                    <defaultValue/>
                    <trim>true</trim>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>taskId</name>
                    <description>任务id</description>
                    <defaultValue/>
                    <trim>true</trim>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>updateStatusData</name>
                    <description>修改任务状态的数据</description>
                    <defaultValue/>
                    <trim>true</trim>
                </hudson.model.StringParameterDefinition>
            </parameterDefinitions>
        </hudson.model.ParametersDefinitionProperty>
    </properties>
    <scm class="hudson.plugins.git.GitSCM" plugin="git@3.12.1">
        <configVersion>2</configVersion>
        <userRemoteConfigs>
            <hudson.plugins.git.UserRemoteConfig>
                <url>${gitUrl}</url>
                <credentialsId>ttt123</credentialsId>
            </hudson.plugins.git.UserRemoteConfig>
        </userRemoteConfigs>
        <branches>
            <hudson.plugins.git.BranchSpec>
                <name>*/${gitBranch}</name>
            </hudson.plugins.git.BranchSpec>
        </branches>
        <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
        <submoduleCfg class="list"/>
        <extensions/>
    </scm>
    <assignedNode>jenkins_slave</assignedNode>
    <canRoam>false</canRoam>
    <disabled>false</disabled>
    <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
    <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
    <triggers/>
    <concurrentBuild>false</concurrentBuild>
    <builders>
        <hudson.tasks.Shell>
            <command>

                . ~/.bash_profile
                mvn clean || true
                mvn install -U -DskipTests  || true

                curl -X POST http://39.102.48.202:8090/saas/createCaseByJunitJar  -F "jarFile=@${WORKSPACE}/target/itest-1.0-SNAPSHOT-fat-tests.jar" -F "projectId=${userId}" -F "url=${saveListUrl}" -F "token=${saveListToken}"  -F "uploadCaseFlag=1"
                eval "${updateStatusData}"
            </command>
        </hudson.tasks.Shell>
    </builders>
    <publishers/>
    <buildWrappers/>
</project>